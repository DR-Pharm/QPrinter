QWidget
{
border:3px outset grey;
border-radius:15px;
}
QPushButton
{
border:3px groove steelblue;
border-radius:15px;
}
QPushButton::checked
{
Padding-left:3px;
Padding-top:3px;
}
QComboBox
{
border:3px groove grey;
border-radius:15px;
}
QComboBox::drop-down
{
border-style:solid;
margin-right:15px;
}
QComboBox::down-arrow
{
image:url(./ico/125.png);
}
QComboBox::down-arrow:on
{
padding-top:3px;
}
QLineEdit
{
border:3px groove grey;
border-radius:15px;
}
QLabel
{
border:0px;
}